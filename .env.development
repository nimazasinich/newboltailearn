# NewBolt AI Learn - Development Environment
# Development-specific configuration

# Security (Development - NOT SECURE)
JWT_SECRET=dev-jwt-secret-not-secure
SESSION_SECRET=dev-session-secret-not-secure
CSRF_SECRET=dev-csrf-secret-not-secure

# Application Settings
NODE_ENV=development
PORT=3000
HOST=0.0.0.0
SERVE_FRONTEND=true

# Database Configuration
DB_PATH=./data/persian_legal_ai.db

# Directory Paths
DATASET_DIRECTORY=./datasets
MODEL_DIRECTORY=./models
CHECKPOINT_DIRECTORY=./checkpoints
EXPORT_DIRECTORY=./exports
LOGS_DIRECTORY=./logs

# HuggingFace Configuration
ENABLE_HF_STARTUP_CHECK=false

# TensorFlow Settings
TF_CPP_MIN_LOG_LEVEL=2

# Development Settings
DEBUG=true

# Vite Development Settings
VITE_API_URL=http://localhost:3000
VITE_BASE_PATH=/

# Redis Configuration
REDIS_URL=redis://redis-dev:6379