{
  "name": "iranian-legal-archive",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Persian Legal Document Archive System with AI Classification and Web Scraping",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "start": "node start.js",
    "start:unified": "node start-unified.js",
    "server": "node --loader ts-node/esm server/index.ts",
    "compile-server": "npx tsc server/index.ts --outDir server --target es2020 --module esnext --moduleResolution node --esModuleInterop --allowSyntheticDefaultImports --skipLibCheck",
    "test": "vitest run --reporter=dot --coverage=false",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:phase2": "node test-phase2.js",
    "test:integration": "playwright test tests/integration/",
    "test:performance": "node tests/performance/load-test.js",
    "docker:build": "docker build -t persian-legal-ai .",
    "docker:run": "docker run -p 3001:3001 persian-legal-ai",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "deploy:staging": "docker-compose -f docker-compose.staging.yml up -d",
    "deploy:production": "docker-compose -f docker-compose.prod.yml up -d",
    "db:backup": "ts-node scripts/db-backup.ts backup",
    "db:list": "ts-node scripts/db-backup.ts list",
    "db:clean": "ts-node scripts/db-backup.ts clean",
    "db:restore": "ts-node scripts/db-backup.ts restore",
    "audit:fix": "npm audit fix --only=prod",
    "test:e2e": "playwright test",
    "test:all": "npm run test && npm run test:integration && npm run test:e2e"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.8.4",
    "@tanstack/react-query-devtools": "^5.8.4",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "@types/bcryptjs": "^2.4.6",
    "axios": "^1.6.1",
    "bcryptjs": "^3.0.2",
    "bert-tokenizer": "^1.1.8",
    "better-sqlite3": "^9.5.1",
    "body-parser": "^1.20.2",
    "chart.js": "^4.5.0",
    "clsx": "^2.0.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "csrf": "^3.1.0",
    "dexie": "^4.2.0",
    "dotenv": "^17.2.2",
    "express": "^4.18.3",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.18.2",
    "framer-motion": "^12.23.12",
    "helmet": "^8.1.0",
    "isomorphic-dompurify": "^2.26.0",
    "jsonwebtoken": "^9.0.2",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.294.0",
    "npm": "^11.6.0",
    "prom-client": "^15.1.3",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.0",
    "recharts": "^3.2.0",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^2.0.0",
    "zod": "^4.1.8",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@tailwindcss/vite": "^4.1.13",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__template": "^7.4.4",
    "@types/babel__traverse": "^7.28.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/estree": "^1.0.8",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/json-schema": "^7.0.15",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.14.202",
    "@types/node": "^24.3.1",
    "@types/prop-types": "^15.7.15",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@types/semver": "^7.5.6",
    "@types/socket.io": "^3.0.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jest": "^30.1.3",
    "playwright": "^1.55.0",
    "postcss": "^8.5.6",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.1.13",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.5",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
